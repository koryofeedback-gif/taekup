{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/ai/taekbot",
      "destination": "/api/ai/taekbot"
    },
    {
      "source": "/api/ai/class-plan",
      "destination": "/api/ai/class-plan"
    },
    {
      "source": "/api/ai/welcome-email",
      "destination": "/api/ai/welcome-email"
    },
    {
      "source": "/api/checkout",
      "destination": "/api/checkout"
    },
    {
      "source": "/api/products-with-prices",
      "destination": "/api/products-with-prices"
    },
    {
      "source": "/api/stripe/publishable-key",
      "destination": "/api/stripe/publishable-key"
    },
    {
      "source": "/api/customer-portal",
      "destination": "/api/customer-portal"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
