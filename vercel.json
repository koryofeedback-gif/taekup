{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/super-admin/:path*",
      "destination": "/api/super-admin?path=:path*"
    },
    {
      "source": "/api/stripe/webhook/:uuid",
      "destination": "/api/stripe/webhook/[uuid]"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
